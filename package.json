{
  "name": "mdl-webpack-boilerplate",
  "title": "Material Design lite boilerplate",
  "version": "1.0.0",
  "description": "Material Design lite + Webpack",
  "main": "lib/index.js",
  "scripts": {
    "images": "cpy ./lib/images/*.* ./www/images/",
    "test": "echo \"Error: no test specified\" && exit 1",
    "bundle": "webpack -p -d --progress --colors",
    "build": "npm-run-all images bundle",
    "serve": "webpack-dev-server --content-base www/ --hot --progress --colors",
    "dev": "npm-run-all  images serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shprink/mdl-webpack-boilerplate.git"
  },
  "author": "shprink <contact@julienrenaux.fr> (http://julienrenaux.fr/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shprink/mdl-webpack-boilerplate/issues"
  },
  "homepage": "https://github.com/shprink/mdl-webpack-boilerplate#readme",
  "dependencies": {
    "lodash": "^4.0.1",
    "material-design-icons": "^2.1.3",
    "material-design-lite": "^1.0.6",
    "npm-run-all": "^3.1.0",
    "riot": "^2.6.2",
    "riotcontrol": "0.0.3",
    "whatwg-fetch": "^1.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.4.1",
    "autoprefixer-loader": "^3.2.0",
    "babel-core": "^6.4.5",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-es2015-riot": "^1.1.0",
    "cpy": "^4.0.1",
    "cpy-cli": "^1.0.1",
    "css-loader": "^0.23.1",
    "file-loader": "^0.8.5",
    "hammerjs": "^2.0.8",
    "html-loader": "^0.4.0",
    "html-webpack-plugin": "^2.7.2",
    "jquery": "^3.1.1",
    "materialize-css": "^0.97.7",
    "node-sass": "^3.4.2",
    "nprogress": "^0.2.0",
    "path": "^0.12.7",
    "resolve-url-loader": "^1.6.0",
    "riotjs-loader": "^3.0.0",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.12",
    "webpack-dev-server": "^1.14.1"
  }
